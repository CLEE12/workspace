<%@ page language="java" contentType="text/html; charset=UTF-8"
    pageEncoding="UTF-8"%>
<% 
	String[] news={"대박이야","무슨소리","영웅진우가 나타났어","미키도"};
    for (int i = 0; i < news.length;i++) {
    	if (i==0) out.println("<font size=6 color=red>");
    	out.println(news[i]+"<br>");
    	if (i==0) out.println("</font>");
    }
%>