package ch16;

import java.io.*;

public class File2Ex {
	public static void main(String[] args) {
		File dir = new File(".");
		//File dir = new File();
		String[] list = dir.list();
		for(int i=0; i<list.length; i++) {
			System.out.println(list[i]);
		}
		
	}

}

/*
1. ch15-src-ch15있는 목록 File2Ex
2. javaSrc 목록 File3Ex
*/